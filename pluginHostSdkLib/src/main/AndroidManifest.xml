<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools"
          package="com.twofortyfouram.locale.sdk.host">

    <permission
        android:name="${applicationId}.com.twofortyfouram.locale.sdk.host.permission.internal"
        android:protectionLevel="signature" />

    <uses-permission android:name="${applicationId}.com.twofortyfouram.locale.sdk.host.permission.internal" />

    <!-- Ignore support library compatibility requirements, which is OK because
         using the library is optional for this SDK -->
    <uses-sdk tools:overrideLibrary="android.support.v4,android.support.v7.appcompat,android.support.graphics.drawable,android.support.mediacompat,android.support.fragment,android.support.fragment,android.support.coreui,android.support.coreutils,android.support.compat"/>

    <application />

</manifest>
